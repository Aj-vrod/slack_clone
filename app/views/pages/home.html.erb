<h1>Welcome to my page</h1>
<p>This is a Slack clone</p>
<div class="container">
  <% if user_signed_in? %>
    <p>Click on the link below to start texting</p>
    <div class="card">
      <%= link_to 'GO TO CHATROOM', chatroom_path(id: 1) %>
    </div>
  <% else %>
    <p>First register to start chatting</p>
    <div class="card">
      <%= link_to 'SIGN UP', new_user_registration_path %>
    </div>
  <% end %>
</div>

